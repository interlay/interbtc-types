                    "max_height": "u32"
                },
                "BtcAddress": {
                    "_enum": {
                        "P2PKH": "H160",
                        "P2SH": "H160",
                        "P2WPKHv0": "H160",
                        "P2WSHv0": "H256"
                    }
                },
                "BtcPublicKey": "[u8; 33]",
